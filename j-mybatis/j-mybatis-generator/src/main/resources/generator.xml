<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="test2" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
		<!-- 去除自动生成的注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" /><!-- 是否取消注释 -->
			<property name="suppressDate" value="true" /> <!-- 是否生成注释代时间戳 -->
		</commentGenerator>
		<!--数据库基本信息 -->
		<!-- com.mysql.cj.jdbc.Driver高版本8是个坑 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/jpatest?serverTimezone=GMT"
			userId="root" password="">
		</jdbcConnection>
		<!--生成实体类的位置以及包的名字 -->
		<javaModelGenerator
			targetPackage="com.jachs.mybatis.entity"
			targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!--生成映射文件存放位置,实际上用不上配置XML文件生成出来把SQL复制到Mapper上 -->
		<sqlMapGenerator targetPackage="com.jachs.mybatis.xml"
			targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!--生成Dao类存放位置,mapper接口生成的位置 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.jachs.mybatis.dao" targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!--生成对应表及类名 -->
		<table tableName="computer" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Computer">
		</table>
		<table tableName="exam" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Exam">
		</table>
		<table tableName="exam_examinees" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Exam_examinees">
		</table>
		<table tableName="examinee" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Examinee">
		</table>
		<table tableName="examinee_exams" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Examinee_exams">
		</table>
		<table tableName="example" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Example">
		</table>
		<table tableName="link" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Link">
		</table>
		<table tableName="my_class" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="My_class">
		</table>
		<table tableName="my_student" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="My_student">
		</table>
		<table tableName="my_subject" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="My_subject">
		</table>
		<table tableName="my_subject_my_student_list" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="My_subject_my_student_list">
		</table>
		<table tableName="node" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Node">
		</table>
		<table tableName="player" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Player">
		</table>
		<table tableName="soft_ware" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Soft_ware">
		</table>
		<table tableName="student" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			enableUpdateByExample="false" domainObjectName="Student">
		</table>
		<table tableName="student_card" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
