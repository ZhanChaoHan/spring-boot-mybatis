<mapper namespace="com.jachs.mybatis.multiple.d2.mapper.StudentcardMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jachs.mybatis.multiple.d2.domain.Studentcard">
        <id column="cardId" property="cardId" />
        <result column="cardMonery" property="cardMonery" />
        <result column="cardName" property="cardName" />
