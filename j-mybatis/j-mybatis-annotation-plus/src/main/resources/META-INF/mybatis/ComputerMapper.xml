<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jachs.mybatis.mapper.ComputerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jachs.mybatis.domain.Computer">
        <id column="computerId" property="computerid" />
        <result column="computerName" property="computername" />
        <result column="computerPrice" property="computerprice" />
        <result column="madeTime" property="madetime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        computerId, computerName, computerPrice, madeTime
    </sql>

</mapper>
